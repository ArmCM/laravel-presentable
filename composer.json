{
    "name": "datacreativa/laravel-presentable",
    "description": "Create presenters for Eloquent Models",
    "keywords": [
        "laravel",
        "presentable"
    ],
    "homepage": "https://github.com/datacreativa/laravel-presentable",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jonathan Zarate",
            "email": "zaratedev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "TheHiveTeam\\Presentable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheHiveTeam\\Presentable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "TheHiveTeam\\Presentable\\PresentableServiceProvider"
            ]
        }
    }
}
